import math 

def f(x):
    if (x == 0 or x == 1):
        return x
    elif (x %2 == 0):
        return 4*f(x//2)
    return f(math.floor(x/2)) + f(math.ceil(x/2)) + 2*g(math.floor(x/2))

def g(x):
    if x == 0:
        return 0
    return 2*g(math.floor(x/2)) + 2*f(math.ceil(x/2))

for i in range(10):
    print("f(" + str(i) +"): " + str(f(i)))

for i in range(10):
    print("g(" + str(i) +"): " + str(g(i)))